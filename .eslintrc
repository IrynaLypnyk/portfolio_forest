{
  "env": {
    "es6": true,
    "commonjs": true,
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [2, 2],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-console": 0,
    "no-unused-vars": ["warn"],
    "comma-dangle": ["error", "always-multiline"]
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "$": true,
    "jQuery": true
  }
}